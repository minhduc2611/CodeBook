    "@nestjs/typeorm": "^9.0.1",
    "@reduxjs/toolkit": "^1.8.4",
    "@types/axios": "^0.14.0",
    "@types/jscodeshift": "^0.11.5",
    "@types/mongodb": "^4.0.7",
    "@types/prettier": "^2.7.0",
